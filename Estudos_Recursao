
#include <stdio.h>
#include <stdlib.h>
int fib(int n){

    if(n == 1){
        return(0);
    }else{
        if(n == 2 ){
            return(1);
        }
    }
        
   return (fib(n-1)+fib(n-2));
   
}
int main(int argc, char** argv) {
    int a = 0;
    
    printf("Informe um valor: ");
    scanf("%d",&a);
    
    int y =  fib(a);
   
    printf("%d",y);
    return (0);

}

